package com.example.container;

public class Friend {

	
	
	
	@Override
	public String toString() {
		// TODO Auto-generated method stub
		return "Friend: " + name;
	}

	private String name;

	
	public Friend(String name) {this.name=name;}
	
	public String getName() {
		return name;
	}

	public void setName(String name) {

		this.name = name;
	}	

}
